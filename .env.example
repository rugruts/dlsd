# Environment Configuration
# Set to 'development', 'staging', or 'production'
NODE_ENV=development

# Firebase Configuration
FIREBASE_API_KEY=your_firebase_api_key
FIREBASE_AUTH_DOMAIN=your_project.firebaseapp.com
FIREBASE_PROJECT_ID=your_project_id
FIREBASE_STORAGE_BUCKET=your_project.appspot.com
FIREBASE_MESSAGING_SENDER_ID=123456789
FIREBASE_APP_ID=1:123456789:web:abcdef123456

# RPC Configuration
EXPO_PUBLIC_GBA_RPC_PRIMARY=https://rpc.gorbagana.wtf/
EXPO_PUBLIC_GBA_RPC_FALLBACK=

# Swap Configuration
EXPO_PUBLIC_SWAP_AGGREGATOR_URL=https://lite-api.jup.ag/
EXPO_PUBLIC_SWAP_API_KEY=your_swap_api_key

# On-Ramp Providers
EXPO_PUBLIC_MOONPAY_ENABLED=false
EXPO_PUBLIC_MOONPAY_API_KEY=your_moonpay_api_key

EXPO_PUBLIC_RAMP_ENABLED=false
EXPO_PUBLIC_RAMP_API_KEY=your_ramp_api_key

EXPO_PUBLIC_TRANSAK_ENABLED=false
EXPO_PUBLIC_TRANSAK_API_KEY=your_transak_api_key

# Feature Flags
EXPO_PUBLIC_ENABLE_THRONE_LINKS=true
EXPO_PUBLIC_ENABLE_RUG_RADAR=false
EXPO_PUBLIC_ENABLE_STAKING=false
EXPO_PUBLIC_ENABLE_SWAPS=true
EXPO_PUBLIC_ENABLE_ON_RAMP=false
EXPO_PUBLIC_ENABLE_BACKUP=true
EXPO_PUBLIC_ENABLE_BIOMETRICS=true
EXPO_PUBLIC_ENABLE_NOTIFICATIONS=true
EXPO_PUBLIC_ENABLE_SENTRY=false
EXPO_PUBLIC_SENTRY_DSN=

# OAuth Configuration (for future zkLogin integration)
EXPO_PUBLIC_GOOGLE_CLIENT_ID=
EXPO_PUBLIC_APPLE_CLIENT_ID=
EXPO_PUBLIC_X_CLIENT_ID=