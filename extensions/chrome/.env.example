# Environment Configuration
# Set to 'development', 'staging', or 'production'
VITE_ENV=development

# Firebase Configuration
VITE_FIREBASE_API_KEY=your_firebase_api_key
VITE_FIREBASE_AUTH_DOMAIN=your_project.firebaseapp.com
VITE_FIREBASE_PROJECT_ID=your_project_id
VITE_FIREBASE_STORAGE_BUCKET=your_project.appspot.com
VITE_FIREBASE_MESSAGING_SENDER_ID=123456789
VITE_FIREBASE_APP_ID=1:123456789:web:abcdef123456

# RPC Configuration
VITE_GBA_RPC_PRIMARY=https://rpc.gorbagana.wtf/
VITE_GBA_RPC_FALLBACK=

# Swap Configuration
VITE_SWAP_AGGREGATOR_URL=https://api.jup.ag
VITE_SWAP_API_KEY=your_swap_api_key

# On-Ramp Providers
VITE_MOONPAY_ENABLED=false
VITE_MOONPAY_API_KEY=your_moonpay_api_key

VITE_RAMP_ENABLED=false
VITE_RAMP_API_KEY=your_ramp_api_key

VITE_TRANSAK_ENABLED=false
VITE_TRANSAK_API_KEY=your_transak_api_key

# Feature Flags
VITE_ENABLE_THRONE_LINKS=true
VITE_ENABLE_RUG_RADAR=false
VITE_ENABLE_STAKING=false
VITE_ENABLE_SWAPS=true
VITE_ENABLE_ON_RAMP=false
VITE_ENABLE_BACKUP=true
VITE_ENABLE_BIOMETRICS=true
VITE_ENABLE_NOTIFICATIONS=true
VITE_ENABLE_SENTRY=false
VITE_SENTRY_DSN=