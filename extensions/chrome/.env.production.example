# ========================================
# DumpSack Wallet - Chrome Extension
# Production Environment
# ========================================
# 
# IMPORTANT: This file contains example values for production.
# Copy this file to .env.production and fill in real values.
# NEVER commit .env.production with real keys to version control!
#
# ========================================

# Environment Configuration
VITE_ENV=production
NODE_ENV=production

# ----------------------------------------
# Supabase Configuration
# ----------------------------------------
# Get these from your Supabase project dashboard:
# https://app.supabase.com/project/YOUR_PROJECT/settings/api

VITE_SB_URL=https://your-project.supabase.co
VITE_SB_ANON_KEY=your_supabase_anon_key_here

# Auth provider (supabase or firebase)
VITE_AUTH_PROVIDER=supabase

# ----------------------------------------
# Auth Redirects
# ----------------------------------------
# For Chrome extension in production, use HTTPS callback URL
# This should match your production domain

VITE_AUTH_REDIRECT=https://dumpsack.xyz/auth/callback

# ----------------------------------------
# RPC Configuration
# ----------------------------------------
# Gorbagana blockchain RPC endpoints

VITE_GBA_RPC_PRIMARY=https://rpc.gorbagana.wtf/
VITE_GBA_RPC_FALLBACK=https://rpc2.gorbagana.wtf/

# Transaction settings
VITE_DEFAULT_COMMITMENT=confirmed
VITE_ENABLE_SIMULATION_PREVIEW=true

# ----------------------------------------
# Swap Configuration
# ----------------------------------------

VITE_SWAP_AGGREGATOR_URL=https://swap-api.gorbagana.wtf
VITE_SWAP_API_KEY=your_swap_api_key_here

# ----------------------------------------
# On-Ramp Configuration
# ----------------------------------------

VITE_MOONPAY_ENABLED=true
VITE_MOONPAY_API_KEY=your_moonpay_api_key_here

# ----------------------------------------
# Feature Flags
# ----------------------------------------

VITE_ENABLE_SWAP=true
VITE_ENABLE_STAKING=true
VITE_ENABLE_NFT=true
VITE_ENABLE_BACKUP=true
VITE_ENABLE_PANIC_BUNKER=true
VITE_ENABLE_ANALYTICS=false

# ----------------------------------------
# Analytics Configuration
# ----------------------------------------

VITE_ANALYTICS_ENABLED=false
VITE_ANALYTICS_TRACKING_ID=

